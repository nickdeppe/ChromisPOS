	<queryString>
		<![CDATA[SELECT 
	sf.STARTTIME,
	sf.SEQUENCE,
	f.NAME AS FEATURENAME,
	r.NAME AS RATINGNAME
FROM 
	SHOWFEATURES AS sf
	INNER JOIN FEATURES AS f ON 
	 sf.FEATUREID = f.ID 
	INNER JOIN RATINGS AS r ON 
	 f.RATINGID = r.ID
 WHERE
 	sf.SHOWID =  $P{ID} 
ORDER BY
	sf.SEQUENCE,
	sf.STARTTIME]]>
	</queryString>
