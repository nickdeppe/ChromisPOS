	<parameter name="ARG" class="java.lang.Object" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
	sh.ID,
	th.NAME AS THEATRENAME,
	sh.STARTDATE,
	sh.ENDDATE,
	sh.REPORTSTARTDATE,
	sh.REPORTENDDATE,
	tl.SHOWDATE,
	pr.NAME AS PRODUCTNAME,
	tl.PRICE - (tl.PRICE * ta.RATE) AS NETPRICE,
	tl.PRICE * ta.RATE AS NETTAX,
	tl.PRICE AS GROSSPRICE,
	1 AS TICKETOPEN,
	SUM( CASE WHEN ti.TICKETTYPE = 0 THEN tl.UNITS ELSE 0 END ) + 1 AS TICKETCLOSE,
	SUM( CASE WHEN ti.TICKETTYPE = 0 THEN tl.UNITS ELSE 0 END ) AS GROSSSOLD,
	SUM( CASE WHEN ti.TICKETTYPE = 1 THEN ABS(tl.UNITS) ELSE 0 END ) AS REFUNDS,
	SUM(tl.UNITS) AS NETSOLD,
	SUM( (tl.PRICE * tl.UNITS) - ( tl.PRICE * ta.RATE * tl.UNITS )) AS NETADMISSIONS,
	SUM( tl.PRICE * ta.RATE * tl.UNITS ) AS NETTAXES,
	SUM(tl.PRICE * tl.UNITS) AS GROSSADMISSIONS
FROM 
	TICKETLINES tl
	INNER JOIN TICKETS ti ON
		tl.TICKET = ti.ID
	INNER JOIN RECEIPTS re ON
		ti.ID = re.ID
	INNER JOIN SHOWS sh ON
		tl.SHOWID = sh.ID
	INNER JOIN THEATRES th ON
		sh.THEATREID = th.ID
	INNER JOIN TAXES ta ON
		tl.TAXID = ta.ID
	INNER JOIN PRODUCTS pr ON
		tl.PRODUCT = pr.ID
GROUP BY
	sh.ID,
	th.NAME,
	sh.STARTDATE,
	sh.ENDDATE,
	sh.REPORTSTARTDATE,
	sh.REPORTENDDATE,
	tl.SHOWDATE,
	pr.NAME,
	tl.PRICE - (tl.PRICE * ta.RATE),
	tl.PRICE * ta.RATE,
	tl.PRICE]]>
	</queryString>
